    },
    "testMatch": [
      "**/__tests__/*.(ts|tsx|js)"
    ]
  },
  "dependencies": {
    "bcryptjs": "^2.4.3",
    "body-parser": "^1.19.0",
    "express": "^4.16.3",
    "jsonwebtoken": "^9.0.0",
    "mongoose": "^5.7.4",
    "passport": "^0.4.0",
    "passport-jwt": "^4.0.0",
    "validator": "^11.1.0"
  },
  "devDependencies": {
    "@types/bcryptjs": "^2.4.2",
    "@types/body-parser": "^1.17.1",
    "@types/express": "^4.16.0",
    "@types/jest": "^24.0.19",
    "@types/jsonwebtoken": "^8.3.4",
    "@types/mongoose": "^5.5.20",
    "@types/passport": "^1.0.1",
    "@types/passport-jwt": "^3.0.2",
    "@types/validator": "^10.11.3",
    "jest": "^24.9.0",
    "nodemon": "^1.18.4",
    "ts-jest": "^24.1.0",
    "ts-node": "^7.0.1",
    "typescript": "^3.1.1"
